{
  "name": "@sveltia/ui",
  "version": "0.31.7",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "github:sveltia/sveltia-ui"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "build:watch": "svelte-kit sync && svelte-package --watch",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "format": "prettier --write .",
    "check": "pnpm run '/^check:.*/'",
    "check:audit": "pnpm audit",
    "check:cspell": "cspell --no-progress",
    "check:svelte": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint .",
    "check:oxlint": "oxlint .",
    "check:stylelint": "stylelint '**/*.{css,scss,svelte}'",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@lexical/code": "^0.38.2",
    "@lexical/dragon": "^0.38.2",
    "@lexical/history": "^0.38.2",
    "@lexical/link": "^0.38.2",
    "@lexical/list": "^0.38.2",
    "@lexical/markdown": "^0.38.2",
    "@lexical/rich-text": "^0.38.2",
    "@lexical/selection": "^0.38.2",
    "@lexical/table": "^0.38.2",
    "@lexical/utils": "^0.38.2",
    "@sveltia/utils": "^0.8.6",
    "lexical": "^0.38.2",
    "prismjs": "^1.30.0",
    "svelte-i18n": "^4.0.1"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/package": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "cspell": "^9.3.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.4.0",
    "eslint-plugin-svelte": "^2.46.1",
    "oxlint": "^1.29.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.94.2",
    "stylelint": "^16.26.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-scss": "^6.12.1",
    "svelte": "^5.43.14",
    "svelte-check": "^4.3.4",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "vite": "^7.2.4",
    "vitest": "^4.0.13"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "typesVersions": {
    ">4.0": {
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
