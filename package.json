{
  "name": "@sveltia/ui",
  "version": "0.27.3",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "github:sveltia/sveltia-ui"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "build:watch": "svelte-kit sync && svelte-package --watch",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "format": "prettier --write .",
    "check": "pnpm run '/^check:.*/'",
    "check:audit": "pnpm audit",
    "check:cspell": "cspell --no-progress",
    "check:svelte": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint .",
    "check:stylelint": "stylelint '**/*.{css,scss,svelte}'",
    "test": "playwright test",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@lexical/code": "^0.32.1",
    "@lexical/dragon": "^0.32.1",
    "@lexical/history": "^0.32.1",
    "@lexical/link": "^0.32.1",
    "@lexical/list": "^0.32.1",
    "@lexical/markdown": "^0.32.1",
    "@lexical/rich-text": "^0.32.1",
    "@lexical/selection": "^0.32.1",
    "@lexical/table": "^0.32.1",
    "@lexical/utils": "^0.32.1",
    "@sveltia/utils": "^0.8.0",
    "lexical": "^0.32.1",
    "prismjs": "^1.30.0"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.22.2",
    "@sveltejs/package": "^2.3.12",
    "@sveltejs/vite-plugin-svelte": "5.1.0",
    "cspell": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^51.3.3",
    "eslint-plugin-svelte": "^2.46.1",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.89.2",
    "stylelint": "^16.21.1",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-scss": "^6.12.1",
    "svelte": "5.35.2",
    "svelte-check": "^4.2.2",
    "svelte-i18n": "^4.0.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "vite": "^7.0.1",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "typesVersions": {
    ">4.0": {
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0",
      "esbuild@<=0.24.2": ">=0.25.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "svelte-preprocess"
    ]
  }
}
